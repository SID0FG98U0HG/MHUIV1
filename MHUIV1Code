local function EWQCH_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	local frame = script.Parent.Parent.Parent.UIFrame
	local button = script.Parent
	local Min = script.Parent.Parent.Parent.MinFrame
	
	button.MouseButton1Click:Connect(function()
		frame.Visible = false
		Min.Visible = true
	end)
end
coroutine.wrap(EWQCH_fake_script)()
local function JSHN_fake_script() -- LoadLabel.loadscript 
	local script = Instance.new('LocalScript', LoadLabel)

	local UI = script.Parent.Parent.Parent.UIFrame
	local SubUI = script.Parent.Parent
	script.Parent.Text = "LOADING"
	wait(5)
	SubUI.Visible = false
	UI.Visible = true
end
coroutine.wrap(JSHN_fake_script)()
